{
  "name": "@repo/db",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "prisma": {
    "schema": "./schema.prisma"
  },
  "scripts": {
    "db:generate": "dotenv -e ../../.env -- prisma generate",
    "db:migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "db:migrate:dev": "dotenv -e ../../.env -- prisma migrate dev",
    "db:migrate:status": "dotenv -e ../../.env -- prisma migrate status",
    "db:push": "dotenv -e ../../.env -- prisma db push",
    "db:studio": "dotenv -e ../../.env -- prisma studio",
    "db:reset": "dotenv -e ../../.env -- prisma migrate reset",
    "db:seed": "dotenv -e ../../.env -- tsx ../../scripts/seed.ts",
    "db:deploy": "dotenv -e ../../.env.prod -- prisma migrate deploy",
    "build": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "dotenv-cli": "^8.0.0",
    "prisma": "^5.22.0",
    "typescript": "^5.4.5"
  }
}
