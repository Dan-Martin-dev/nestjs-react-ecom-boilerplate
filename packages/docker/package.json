{
  "name": "@repo/docker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "docker-compose -f ../../docker-compose.dev.yml --env-file ../../.env.dev up",
    "dev:down": "docker-compose -f ../../docker-compose.dev.yml --env-file ../../.env.dev down",
    "prod": "docker-compose -f ../../docker-compose.prod.yml --env-file ../../.env.production up -d",
    "prod:down": "docker-compose -f ../../docker-compose.prod.yml --env-file ../../.env.production down"
  }
}
