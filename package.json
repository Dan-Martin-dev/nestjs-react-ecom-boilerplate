{
  "name": "nestjs-reactjs-ecom-boilerplate",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "db:generate": "turbo db:generate",
    "db:migrate": "pnpm --filter @repo/db db:migrate",
    "db:studio": "pnpm --filter @repo/db db:studio",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,yaml}\""
  },
  "devDependencies": {
    "@tanstack/router-cli": "^1.130.15",
    "@types/node": "^22.10.7",
    "@types/passport-facebook": "^3.0.3",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-instagram": "^1.0.5",
    "dotenv-cli": "^8.0.0",
    "prettier": "^3.2.5",
    "turbo": "^2.5.5",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@9.1.4",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.665.0",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "mime": "^4.0.4",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-instagram": "^1.0.0",
    "sharp": "^0.33.5"
  }
}